{
  "packages": ["packages/*"],
  "version": "independent",
  "command": {
    "publish": {
      "verifyAccess": false,
      "allowBranch": ["master", "next"],
      "message": "chore(release): publish",
      "noVerifyAccess": true,
      "conventionalCommits": true,
      "distTag": "latest"
    }
  }
}
